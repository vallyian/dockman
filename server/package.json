{
    "name": "@dockman/server",
    "version": "0.0.0",
    "private": true,
    "license": "ISC",
    "description": "dockman server",
    "keywords": [
        "dockman",
        "server"
    ],
    "author": "vallyian@gmail.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/vallyian/dockman.git"
    },
    "engines": {
        "node": ">=16 <17",
        "npm": ">=8 <9"
    },
    "type": "module",
    "scripts": {
        "prebuild": "node -e \"fs.rmSync('bin', { recursive: true, force: true })\"",
        "build": "npx tsc --project tsconfig.app.json --outDir bin",
        "test": "npx ts-node --project tsconfig.spec.json node_modules/jasmine/bin/jasmine --config=jasmine",
        "start": "NODE_ENV=development PORT=55556 npx ts-node-dev --project tsconfig.app.json --respawn --transpile-only -- src/index"
    },
    "dependencies": {
        "body-parser": "^1.20.0",
        "express": "^4.18.0",
        "helmet": "^4.6.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/express": "^4.17.13",
        "@types/jasmine": "^3.10.6",
        "@types/node": "^16.11.32",
        "jasmine": "^3.99.0",
        "jasmine-reporters": "^2.4.0",
        "jasmine-spec-reporter": "^7.0.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.6.4"
    }
}
