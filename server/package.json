{
    "name": "@dockman/server",
    "version": "0.0.0",
    "private": true,
    "license": "ISC",
    "description": "dockman server",
    "keywords": [
        "dockman",
        "server"
    ],
    "author": "vallyian@gmail.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/vallyian/dockman.git"
    },
    "engines": {
        "node": ">=14 <15"
    },
    "scripts": {
        "build": "node node_modules/typescript/bin/tsc --project tsconfig.app.json --outDir ./bin",
        "test": "node node_modules/ts-node/dist/bin --project tsconfig.spec.json node_modules/jasmine/bin/jasmine --config=jasmine",
        "start": "NODE_ENV=development node node_modules/ts-node-dev/lib/bin --project tsconfig.app.json --respawn --transpile-only -- src/index"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "helmet": "^4.6.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.1",
        "@types/express": "^4.17.13",
        "@types/jasmine": "^3.10.1",
        "@types/node": "^14.17.29",
        "jasmine": "^3.10.0",
        "jasmine-reporters": "^2.4.0",
        "jasmine-spec-reporter": "^7.0.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.4"
    }
}
